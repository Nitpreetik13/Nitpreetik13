        "def tclean(text1):\n",
        "  text1=re.sub(\"#[A-Za-z0-9_]+\",\"\", text1)\n",
        "  return text1"
      ],
      "metadata": {
        "id": "AcFP7_nmLRaF"
      },
      "execution_count": 161,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "REMOVING EMOJI'S (USING UTF-8)"
      ],
      "metadata": {
        "id": "JtvcKbvgAe0c"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def deEmojify(a):\n",
        "    regrex_pattern = re.compile(pattern = \"[\"\n",
        "        u\"\\U0001F600-\\U0001F64F\"  \n",
        "        u\"\\U0001F300-\\U0001F5FF\"  \n",
        "        u\"\\U0001F680-\\U0001F6FF\"  \n",
        "        u\"\\U0001F1E0-\\U0001F1FF\"  \n",
        "                           \"]+\", flags = re.UNICODE)\n",
        "    return regrex_pattern.sub(r'', a)"
      ],
      "metadata": {
        "id": "nWMRQFuHMo_k"
      },
      "execution_count": 162,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def cleaning(df):\n",
        "  df['c3']=df['c1'].apply(clean)\n",
        "  df['c3']=df['c3'].apply(tclean)\n",
        "  df['c3']=df['c3'].apply(deEmojify)"
      ],
